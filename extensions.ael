macro dial-EU( ext ) {
// карточки 1,2 - Вся європа (безлім)
// карточки 3,4 - Польща (безлім)


switch (${ext}) {
	pattern +48.: 
	// call to PL - line search order 4,3 - 2,1
	Dial(SIP/PL/2${ext},60);
	Dial(SIP/PL/1${ext},60);
	break;

	default: 
	// call to EU - line search order 2,1
	Dial(SIP/PL/1${ext},60);
	break;

}; // switch

};



macro ats( ext ) {
	Set(fname=${STRFTIME(${EPOCH},,%Y%m%d%H%M)}-${CALLERID(number)}-${ext});
	MixMonitor(/var/log/asterisk/records/${fname}.wav);
	Dial(SIP/linksys1,60,D(${ext}));
	Dial(SIP/linksys2,60,D(${ext}));
	Dial(SIP/linksys3,60,D(${ext}));
	Dial(SIP/linksys4,60,D(${ext}));

};

macro call-eu( ext ) {
// Пробуєм через брідж в PL, а якщо невдалося - через System Telecom

				Set(fname=${STRFTIME(${EPOCH},,%Y%m%d%H%M)}-${CALLERID(number)}-${ext});
                                MixMonitor(/var/log/asterisk/records/${fname}.wav);
				&dial-EU(${ext});
				// Dial(SIP/systelecom/${ext:1}); // відрізаєм +				
};

macro call-eu-gw( ext ) {
// тільки через брідж в PL

                                Set(fname=${STRFTIME(${EPOCH},,%Y%m%d%H%M)}-${CALLERID(number)}-${ext});
                                MixMonitor(/var/log/asterisk/records/${fname}.wav);
                                &dial-EU(${ext});
};


macro callout( chan, ext) {
				Set(fname=${STRFTIME(${EPOCH},,%Y%m%d%H%M)}-${CALLERID(number)}-${ext});
				MixMonitor(/var/log/asterisk/records/${fname}.wav);
				Dial(${chan}/${ext});
};



