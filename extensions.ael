context wait-extn {
			#   => jump ${Dex}@probiz-eu;
			_X! => jump ${Dex}${EXTEN}@${ExContext};
			s   => WaitExten(13);
			t   => jump ${Dex}@probiz-eu;
};


context ext-ukr-guest {
			 _2XX           => &ats(${EXTEN});
	                 _[456]XX        => &callint(${EXTEN});
			 _X!		=> {
						Set(Dex=${EXTEN});
						jump s@wait-extn;
			 };
			_XXX    =>  { zapateller;
                                                 Hangup();
                                               };

			s =>  {
				Answer();
	                        Set(ExContext=ext-ukr-guest);
				Set(Dex=);
				jump s@wait-extn;
			};
};

